<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.training.aplikasiimplicitintent">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AlarmActivity" />
        <activity android:name=".AudioRecorderActivity" />
        <activity android:name=".AudioManagerActivity" />
        <activity android:name=".WifiActivity" />
        <activity android:name=".SmsActivity" />
        <activity android:name=".PhoneActivity" />
        <activity android:name=".BluetoothActivity" />
        <activity android:name=".TtsActivity" />
        <activity android:name=".CameraActivity" />
        <activity android:name=".VideoActivity" />
        <activity android:name=".EmailActivity" />
        <activity android:name=".BrowserActivity"></activity>
    </application>

</manifest>