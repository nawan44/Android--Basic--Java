<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androtrain.www.navigationdrawer">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.androtrain.www.navigationdrawer.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.androtrain.www.navigationdrawer.activity.AboutUsActivity"
            android:label="@string/activity_title_about_us" />
        <activity
            android:name="com.androtrain.www.navigationdrawer.activity.PrivacyPolicyActivity"
            android:label="@string/activity_title_privacy_policy" />

    </application>

</manifest>
